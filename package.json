{
  "name": "aleph-fetch",
  "version": "4.0.0",
  "description": "Custom Node.js wrapper methods for the Aleph X-Services API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:ugent-library/aleph-fetch.git",
  "author": "Koen Verheyen <koen.verheyen@inuits.eu>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist/ && tsup index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "non-dirty-check": "git diff --quiet --exit-code"
  },
  "precommit": [
    "build",
    "non-dirty-check"
  ],
  "dependencies": {
    "dotenv": "^16.0.3",
    "jstoxml": "^3.2.5",
    "lodash": "^4.17.21",
    "xml2js": "^0.4.23",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@fastify/pre-commit": "^2.0.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/jstoxml": "^2.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/xml2js": "^0.4.11",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
